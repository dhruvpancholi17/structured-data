  (let [[[x1 y1] [x2 y2]] rectangle]
    (cond
      (= (- x2 x1) (- y2 y1)) true
      :else false)
    ))
(defn area [rectangle]
  (let [[[x1 y1] [x2 y2]] rectangle]
    (* (- x2 x1) (- y2 y1))
    ))
(defn contains-point? [rectangle point]
  (let [[[x1 y1] [x2 y2]] rectangle
        [x3 y3] point]
    (cond
      (and (<= x1 x3 x2) (<= y1 y3 y2)) true
      :else false)
    ))
(defn contains-rectangle? [outer inner]
  (let [[[x1 y1] [x2 y2]] outer
        [[x3 y3] [x4 y4]] inner]
    (cond
      (and (<= x1 x3 x4 x2) (<= y1 y3 y4 y2)) true
      :else false)
    ))
